 !  Static Global Variables declared in yoesrta16.F90
 EQUIVALENCE (KAC(1,1,1,1),ABSA(1,1)), (KBC(1,13,1),ABSB(1,1))

 !$ACC DECLARE CREATE(KAC, ABSA, KBC, ABSB, SELFREFC, FORREFC, SFLUXREFC)
 
 !$OMP DECLARE TARGET(SELFREFC, FORREFC, SFLUXREFC)

 !  Initialized and copied to device in srtm_cmgb16.F90
 !$ACC UPDATE DEVICE(KAC, KBC, SELFREFC, FORREFC, SFLUXREFC)

 !$OMP TARGET UPDATE TO(SELFREFC, FORREFC, SFLUXREFC)
 !$OMP TARGET ENTER DATA MAP(TO: KAC, KBC)
